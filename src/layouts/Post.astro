---
const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="description" content={title} />
		<title>{title}</title>
	</head>
	<body>
      <slot />
	</body>
</html>

<style>
        img {
            width: 200px;
        }
    </style>
